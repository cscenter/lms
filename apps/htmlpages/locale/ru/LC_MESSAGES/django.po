#: apps/htmlpages/admin.py:24
msgid "Advanced options"
msgstr "Расширенные настройки"

#: apps/htmlpages/apps.py:7
msgid "Flat Pages"
msgstr "Простые страницы"

#: apps/htmlpages/forms.py:10 apps/htmlpages/models.py:11
msgid "URL"
msgstr "URL"

#: apps/htmlpages/forms.py:11
msgid ""
"Example: '/about/contact/'. Make sure to have leading and trailing slashes."
msgstr ""
"Пример: '/about/contact/'. Убедитесь, что ввели начальную и конечную косые "
"черты."

#: apps/htmlpages/forms.py:14
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Значение должно состоять только из букв, цифр и символов точки, "
"подчеркивания, тире, косой черты и тильды."

#: apps/htmlpages/forms.py:27
msgid "URL is missing a leading slash."
msgstr "В начале URL отсутствует косая черта"

#: apps/htmlpages/forms.py:34
msgid "URL is missing a trailing slash."
msgstr "В конце URL отсутствует косая черта"

#: apps/htmlpages/forms.py:51
#, fuzzy, python-format
#| msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgid "Htmlpage with url %(url)s already exists for site %(site)s"
msgstr "Простая страница с адресом %(url)s уже существует для сайта %(site)s"

#: apps/htmlpages/models.py:12
msgid "title"
msgstr "заголовок"

#: apps/htmlpages/models.py:13
msgid "content"
msgstr "содержимое"

#: apps/htmlpages/models.py:14
msgid "template name"
msgstr "имя шаблона"

#: apps/htmlpages/models.py:16
#, fuzzy
#| msgid ""
#| "Example: 'flatpages/contact_page.html'. If this isn't provided, the "
#| "system will use 'flatpages/default.html'."
