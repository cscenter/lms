<script>
    function addAchivement(userId, code) {
        $(`#user-card-${userId} .achievements`).append(`
            <div class="achievements__item" data-toggle="tooltip" title="${achievements[code]}">
                <svg class="sprite-img _${code}" xmlns:xlink="http://www.w3.org/1999/xlink"><use xlink:href="#${code}"></use></svg>
            </div>
        `);
        $('[data-toggle="tooltip"]').tooltip({
            animation: false,
            placement: "auto",
            delay: {"show": 100, "hide": 0}
        });
    }

    var achievements = {
        "spring": "Все практики только весной",
        "grampycat": "Ни разу не записывался на праздник до этого и не участвовал в НГ викторине",
