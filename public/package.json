{
  "name": "csc",
  "version": "1.0.0",
  "description": "Webpack",
  "scripts": {
    "type-check": "tsc",
    "type-check:watch": "npm run type-check -- --watch",
    "gulp": "gulp --gulpfile gulp/gulpfile.babel.js",
    "gulp:build": "gulp --gulpfile gulp/gulpfile.babel.js build",
    "start": "webpack-dev-server --progress --config webpack/v1.config.js",
    "dev2": "APP_VERSION=v2 webpack-dev-server --progress --config webpack/v2.config.js",
    "prod": "NODE_ENV=production webpack --config webpack/v1.config.js --progress --display-error-details --display-modules",
    "prod2": "APP_VERSION=v2 NODE_ENV=production BABEL_ENV=production webpack --config webpack/v2.config.js",
    "msg:collect": "NODE_ENV=production BABEL_ENV=production i18next-scanner --config i18next-scanner.config.js",
    "css-lint": "stylelint --syntax scss \"src/**/*.scss\"",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "jest --no-cache --config src/v2/js/jest.config.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand"
  },
  "repository": {
    "type": "git",
